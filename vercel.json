{
    "version": 2,
    "builds": [
     {
      "src": "index.js",
      "use": "@vercel/node"
    
     },
     {
        "src": "build/**",
      "use": "@vercel/static"
     }
    ],
    "routes": [
     {
      "src": "/api/(.*)",
      "dest": "/index.js"
     },
     {
        "src": "/",
        "dest": "build/index.html"
       },
       {
        "src": "/login",
        "dest": "build/index.html"
       },
       {
        "src": "/signup",
        "dest": "build/index.html"
       },
       {
        "src": "/voteparty",
        "dest": "build/index.html"
       },
       {
        "src": "/admin",
        "dest": "build/index.html"
       },
       {
        "src": "/votercard",
        "dest": "build/index.html"
       },
       
       {
        "src": "/dashboard",
        "dest": "build/index.html"
       },
       {
        "src": "/downloadvotercard",
        "dest": "build/index.html"
       },
       {
        "src": "/updatevotercard",
        "dest": "build/index.html"
       },
       {
        "src": "/votingdata",
        "dest": "build/index.html"
       },
       {
        "src": "/forgetpassword",
        "dest": "build/index.html"
       },
       {
        "src": "/displayresult",
        "dest": "build/index.html"
       },
       {
        "src": "/previousresult",
        "dest": "build/index.html"
       },
       {
        "src": "/About Us",
        "dest": "build/index.html"
       },
       {
        "src": "/Contact Us",
        "dest": "build/index.html"
       },
       {
        "src": "/Privacy Policy",
        "dest": "build/index.html"
       },
       {
        "src": "/Terms of Service",
        "dest": "build/index.html"
       },
     {
        "src": "/(.+)",
        "dest": "build/$1"
       }
    ]
   }
